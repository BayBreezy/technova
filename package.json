  "scripts": {
    "build": "nuxt build",
    "clean": "rm -rf .nuxt dist node_modules package-lock.json && npm install",
    "dev": "nuxt dev --host",
    "format": "npx --yes prettier --write .",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "ui:add": "npx --yes ui-thing@latest add",
    "prepare": "husky"
  },
