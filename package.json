    "@vite-pwa/nuxt": "^0.10.5",
    "nuxt": "^3.13.2",
    "nuxt-marquee": "^1.0.4",
    "nuxt-module-hotjar": "^1.3.2",
    "nuxt-security": "^2.0.0",
    "nuxt-translation-manager": "^1.1.1",
    "radix-vue": "^1.9.6",
    "tailwind-variants": "^0.2.1",
